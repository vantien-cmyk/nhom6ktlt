file_name = "romeo.txt"
unique_words = []

try:
    with open(file_name, 'r') as file_handle:
        for line in file_handle:
            words = line.rstrip().split()
            for word in words:
                if word not in unique_words:
                    unique_words.append(word)

    unique_words.sort()
    print(unique_words)

except FileNotFoundError:
    print(f"Lỗi: Không tìm thấy tệp '{file_name}'.")
